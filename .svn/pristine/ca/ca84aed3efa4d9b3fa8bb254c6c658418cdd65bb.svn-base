#moquu_top{
    display:none;
    z-index:2;
    width:50px;
    height:50px;
    right:10px;
    bottom:85px;
    position:fixed;
    cursor:pointer;
    _margin-top:516px;
    background:url(../../images/back-top.png) 0 -101px;	
    _position:absolute;
    _bottom:auto;
