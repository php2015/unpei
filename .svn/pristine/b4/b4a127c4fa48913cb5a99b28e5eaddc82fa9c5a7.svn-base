*{ margin:0 ; padding:0; font-size:12px; color:#666;font-family:宋体}

a{ outline:none}
table /* 设置table 的样式*/
{
    border-collapse:collapse;
    border-spacing:0;
}
fieldset, img
{
    border:0;
}
em, strong
{
    font-style:normal;
    font-weight:normal;
}
li
{
    list-style:none;
}
h1, h2, h3, h4, h5, h6
{
    font-size:100%;
    font-weight:normal;
}
a{ text-decoration:none;}
a:hover{color:#ec8051}
