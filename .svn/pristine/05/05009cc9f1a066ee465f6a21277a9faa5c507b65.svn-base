@charset "utf-8";
/* base */
body{font-family:"微软雅黑",Arial, Helvetica, sans-serif;font-size: 12px;font-style: normal;line-height: 22px;font-weight: normal;font-variant: normal;color:#666;}
body,ul,dl,dd,dt,ol,li,p,h1,h2,h3,h4,h5,h6,form,fieldset,table,img,td,div,input{margin:0;padding:0;}
table{border-collapse:collapse;border-spacing:0;}
a{outline-style:none;color:#666;text-decoration:none;border:0px;}
a:hover{text-decoration:none;color:#04999a;}
input,select,textarea{font-size:12px;font-family:Arial, Helvetica, sans-serif;color:#666}
img{border:0;}
li{list-style-type:none;vertical-align:0}
.hidden{display:none;}
.fleft{float:left}
.fright{float:right}
.fleft_impt{float:left!important;}
.pr{position:relative;}
.pa{position:absolute}
.clear{clear:both;height:0;line-height:0;overflow:hidden;font-size:0;}
textarea{resize: none;}
.f12{font-size:12px;}
.f14{font-size:14px;}
.f16{font-size:16px;}
.f18{font-size:18px;}
.f22{font-size:22px;}
.f24{font-size:24px;}
.fbold{font-weight:700}
.nav2 li.lineh10{ line-height:10px;*line-height:15px}
.nav2 li.lineh10 a{ margin-top:10px; *margin-top:5px}



/* header */
.head{ background:url(../../images/newer/bj_line.jpg) #fff repeat-x left top; height:110px; width:100%;position:relative; z-index:4100; min-width:1000px }
.logo{width:114px;height:18px;}
.header{padding-top:16px;height:96px;width:1000px;margin:0 auto;_float:left;position:relative;}
.nav{margin-left:50px;display:inline;height:47px; line-height:47px;_width:560px;_float:left;}
.nav li{float:left;font-size:18px;cursor:pointer;margin-right:40px;_margin-right:30px;height:47px;display:inline}
.nav li a,.nav li a:hover{color:#343434}

.header .sreach {width:202px;float:right}
.head-nav{ height:35px; background:#fff; position:relative; z-index:200}
.head-nav-info{ margin:0 auto; width:1000px; line-height:35px}
.head-nav-info ul li{ float:left;margin-right:100px; cursor:pointer}
.head-nav-info ul li.active{ color:#0164c1}

.nav_1{height:40px;line-height:40px;}
.nav_1 a,.nav_1 span{float:left;margin-right:5px;color:#74848a}
.nav_1 a:hover{text-decoration:underline}
.nav_1 span{font-family:宋体}

.nav2{height:55px;width:963px;position:absolute;left:0;top:62px;_top:62px;}
.nav2 li{font-size:12px;line-height:48px;height:48px;margin-right:0;float:left;}
.nav2 li.r_nav_none{background:none}
.nav2 li a{color:#fff;display:block;margin-bottom:5px;text-align:center; padding:0px 40px; font-size:14px}
.nav2 li a:hover{color:#fff;}
.nav2 li.r_i_xuan_z a,.nav2 li.r_i_xuan_z a:hover{color:#0164c1;}
.nav2 li.r_i_xuan_z{background:url(../../images/newer/r_i_xuan.gif) repeat-x 5px 5px}

.r_ind_nav span{color:#00a9a7;height:47px;display:block;background:url(../../images/newer/r_ind_bi.gif) no-repeat center bottom;float:left;white-space:normal}
.r_ind_nav span a,.r_ind_nav span a:hover{color:#0164c1;}
.nav2{display:none}
.r_ind_nav .nav2{display:block}
.r_i_xuan_z .nav2_1{display:block}
.nav2_1{padding-top:30px;display:none;position:absolute;left:-5px;}


.r_pohne{background:url(../../images/newer/r_i_img.gif) no-repeat center left;height:32px;line-height:32px;padding-left:40px;font-size:15px;float:right;margin:5px 20px 0 0;display:inline;_margin-top:-5px;}
.nav2_hover{height:50px;overflow:hidden;}
.nav2_hover ul{width:720px;margin:0 auto}
.nav2_hover ul li{width:auto;padding:0 22px;}
.nav2_hover li a{text-align:center}
.nav2_hover li.r_i_xuan_z{background:url(../../images/newer/nav_hover.png) no-repeat left top;}
.nav2_hover li.r_i_xuan_z a,.nav2_hover li.r_i_xuan_z a:hover{color:#fff;}



.width1000{ width:1000px; margin:0 auto; position:relative; z-index:1000}
.light-div{position:absolute;  z-index:3; border:3px solid #f27300}
.link{font-size:16px; font-weight:bold; color:#0e90ff
; position:absolute; right:10px ; bottom:10px; font-weight:bold}
.shade{background: none repeat scroll 0 0 #676767;left: 0;filter:alpha(opacity=50);        -moz-opacity:0.5;  -khtml-opacity: 0.5;opacity: 0.5;  position:fixed;top:0px;width: 100%;z-index:2;}

