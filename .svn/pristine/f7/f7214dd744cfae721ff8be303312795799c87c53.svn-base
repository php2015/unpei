@charset "utf-8";html{color:#4d4d4d;background:#fff; font-size:12px}
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,textarea,p,blockquote,th,td,hr,button,article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section{margin:0;padding:0;}
article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section{display:block;}
body,button,input,select,textarea{font:12px/1.5 tahoma,arial,\5b8b\4f53;}
input,select,textarea{font-size:100%;}
table{border-collapse:collapse;border-spacing:0;}
th{text-align:inherit;}
fieldset,img,a img{border:0;}
iframe{display:block;}
abbr,acronym{border:0;font-variant:normal;}
del{text-decoration:line-through;}
address,caption,cite,code,dfn,em,th,var{font-style:normal;font-weight:500;}
ol,ul,li{list-style:none;}
a{ text-decoration:none; color:#4d4d4d}
caption,th{text-align:left;}
h1,h2,h3,h4,h5,h6{font-size:100%;font-weight:500;}
.float_l{ float:left} 
.float_r{ float:right} 
.width180{ width:180px}
.width190{ width:190px}
.width250{ width:250px}
.width200{ width:200px}
.width400{width:400px}
.m_top10{ margin-top:10px}
.m_left5{ margin-left:5px}
.red{ color:red}
.padding15{ padding:15px}
.padding10{ padding:10px}
.width795{ width:795px}
.m_left30{ margin-left:30px}
.clear{ clear:both}
.fw{ font-weight:bold;}
.wz{ font-size:14px; line-height:24px; }
.border{border:1px solid #ccc}
.bor_bottom{border-bottom:1px dashed #ccc}
.ui-container{ margin:0 auto;  min-height:100%;height: auto !important;height: 100%;position:relative}
.global-head{ margin:0px auto; width:1000px; height:100px;  }
.global-head-area{ line-height:40px; margin:10px auto}
.logo{ width:120px}
.global-toplink  a{  padding:0 5px; }
.nav{ height:38px; border:1px solid #1c6cbe;border-radius:5px; background:#4096ee}
.nav li{ float:left; margin:5px 10px 0px 15px; height:35px; line-height:30px}
.nav li a{ padding:0px 10px ; font-size:14px; font-weight:bold; color:#fff}
.nav li.current{ background:#fff}
.nav li.current a{ color:#4d4d4d}
.contents{ margin:10px auto; width:1000px}
.box{ border:1px solid #c9c8c9}
.box_lm{ height:35px; line-height:35px; background:url(../../images/helpcenter/lm-bg.jpg) repeat-x; border-bottom:1px solid #c9c8c9; font-size:14px; font-weight:bold; text-indent:15px; color:#656565}
.side-normal li{ line-height:40px; height:40px; background:url(../../images/helpcenter/tubiao.png) no-repeat -184px 5px; text-indent:45px}
.box_info p{ line-height:24px; margin-bottom:5px}
.box-sousuo{ height:35px; border:1px solid #ccc; background:#ebebeb}
.sousuo{background:url(../../images/helpcenter/tubiao.png) no-repeat #fff -188px -43px; border:1px solid #ccc;width:310px; margin:3px; height:25px; line-height:25px; text-indent:30px ; color:#a5a5a5; outline:none; padding:1px}
.botton{ background:url(../../images/helpcenter/tubiao.png) no-repeat -167px -84px; height:30px; width:70px; border:none; color:#fff; font-weight:bold; font-size:14px;}
.textarea{ vertical-align:top; width:600px}
.footer{ text-align:center; background:#b8b6b7;}
.footer{ height:85px}
.footer_info{  line-height:24px; text-align:center;color:#444; padding-top:10px}
.footer_info a{  color:#565656; padding-right:5px;}
.footer_info a:hover{color:#0063be}
