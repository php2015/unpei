.bigautocomplete-layout{display:none;background-color:#FFFFFF;border:2px solid #ec8051;position:absolute;z-index:100;max-height:220px;overflow-x:hidden;overflow-y:auto; }
.bigautocomplete-layout table{border-collapse:collapse;border-spacing:0;background:none repeat scroll 0 0 #FFFFFF;width:100%;cursor:default;}
.bigautocomplete-layout table tr{background:none repeat scroll 0 0 #FFFFFF;}
.bigautocomplete-layout .ct{background:none repeat scroll 0 0 #D2DEE8 !important;}
.bigautocomplete-layout div{word-wrap:break-word;word-break:break-all;padding:1px 5px;}
.content1{}
.dh_lm{ height:37px; line-height:35px; background:url(../../images/lm-bg.png) repeat-x; text-indent:20px; color:#fff; font-weight:bold; font-size:14px; letter-spacing:1px}
.dh_info{ border:1px solid #ec8051; border-top:none; clear:both; padding:2px;height:338px;overflow: hidden }
.dh_xq{  background:#FFF; width:460px; padding:0px 18px; margin-top:2px;  }

.biglm{ font-size:14px ; font-weight:bold; color:#444; margin-left:5px; line-height:25px; color:#e6874d}
.smalllm{ border-bottom:1px dashed #c2c2c2}
.smalllm ul{ padding-bottom:9px;padding-left:0px}
.smalllm li{ margin: 3px 5px; float:left;  white-space:nowrap;}
.smalllm li a{ color:#888;}
.smalllm li a:hover{ color:#ec8051}
.last_div{ border-bottom:none}
.prand,.agency{ width:490px; }


.limit-box{position:relative;margin:10px auto 0px;height:200px;overflow:hidden;border:1px solid #e6874d;border-top:none}
/* limit-buy-hd */
.limit-buy-hd{position:relative;height:37px;background:url(../../images/lm-bg.png) repeat-x;line-height:37px; }
.limit-buy-hd h2{float:left;overflow:hidden;display:inline;width:200px;font-size:14px;color:#fff; text-indent:20px; letter-spacing:1px; font-weight:bold}
/* limit-buy-nav */
.limit-buy-nav{float:right;width:60px;margin:7px 10px 0 0}



/* limit-buy-bd */

/* btn-prev */

.products{float:left;width:490px}
.jxs-li{float:left;position:relative;width:140px;height:155px;padding:5px 11px 0; border-right:1px solid #f0f0f0; }

.jxs-li .pic{overflow:hidden;position:relative;width:90px;height:90px;margin:0 auto 4px}
.jxs-li .pic img{width:90px;height:90px}
.jxs-li .tel{text-align:center}
.jxs-li .name{overflow:hidden;height:28px;margin:0 0 2px;line-height:14px;text-align:center;  font-weight:bold}
.name a{color:#3364ca;}




.btndiv{width:100%;float:left;height:28px;position:relative;}
.btndiv strong{float:left;padding-left:10px;padding-top:4px;}
a.abtn{display:block;height:22px;width:24px;position:absolute;overflow:hidden;background:url(../../images/btn_1.png) no-repeat;text-indent:-999em;outline:none;}
a.aleft{right:40px;background-position:0 -32px;}
a.agrayleft{cursor:default;background-position:0 0;}
a.aright{right:10px;background-position:-34px 0;}
a.agrayright{cursor:default;background-position:-34px -32px;}
.uldiv1,uldiv2{width:490px; height:200px}
.scrollcontainer{height:163px;overflow:hidden;position:relative;margin:0 auto; border:1px solid #ec8051; border-top:none; }
.scrollcontainer ul{width:10000px;position:absolute;left:0px;top:0px;}
.scrollcontainer li{width:490px;height:163px;float:left;}
